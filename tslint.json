{
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": [
			true,
			"allow-fast-null-checks",
			"allow-new"
		],
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": true
	},
	"defaultSeverity": "warning"
}